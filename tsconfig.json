{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es6",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./dist/types",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true
  },
  "include": [
    "registry",
  ]
}
